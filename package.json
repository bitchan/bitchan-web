{
  "name": "nekogrid-web",
  "version": "0.0.0",
  "description": "Browser nekogrid client",
  "scripts": {
    "test": "mocha && jsxhint .",
    "clean": "rm -rf dist",
    "build-dev": "npm run clean && webpack",
    "build-release": "npm run clean && NODE_ENV=production webpack",
    "start": "webpack-dev-server --port 4444 --history-api-fallback --colors",
    "prepublish": "bower install && npm run build-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nekogrid/nekogrid-web.git"
  },
  "keywords": [
    "nekogrid"
  ],
  "author": "Kagami Hiiragi",
  "license": "CC0",
  "bugs": {
    "url": "https://github.com/nekogrid/nekogrid-web/issues"
  },
  "homepage": "https://github.com/nekogrid/nekogrid-web",
  "devDependencies": {
    "bower": "^1.3.12",
    "chai": "*",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.1.0",
    "jsx-loader": "^0.12.2",
    "jsxhint": "*",
    "mocha": "*",
    "sass-loader": "^0.3.1",
    "traceur-loader": "^0.6.3",
    "webpack": "^1.4.13",
    "webpack-dev-server": "^1.6.6"
  },
  "dependencies": {
    "dexie": "^1.0.0",
    "flux": "^2.0.1",
    "react": "^0.12.1"
  }
}
